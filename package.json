{
  "name": "sequelize-mocks",
  "version": "0.1.0",
  "description": "Generate sequelize models based on sequelize-typescript annotations",
  "main": "index.js",
  "repository": "https://github.com/maxjoehnk/sequelize-mocks",
  "author": "Max JÃ¶hnk <maxjoehnk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf '*.{d.ts,js}'",
    "prebuild": "npm run clean",
    "build": "tsc --outDir .",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run check",
    "prepack": "npm run check",
    "format": "dprint fmt",
    "format.check": "dprint check",
    "check": "npm run build && npm run lint && npm run test"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "faker": "^5.5.3"
  },
  "peerDependencies": {
    "sequelize": "^5.22.4",
    "sequelize-typescript": "^1.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "jest": "^27.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "^5.22.4",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^5.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.ts"
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/ban-types": 0
    }
  }
}
